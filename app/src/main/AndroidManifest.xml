<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.friendr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="android.friendr.FindNewInterest"/>
        <activity android:name="android.friendr.CreateEvent" />
        <activity android:name="android.friendr.GroupMainPage" />
        <activity android:name="android.friendr.CreateGroup" />
        <activity android:name="android.friendr.FindNewGroup" />
        <activity android:name="android.friendr.WelcomeActivity" />
        <activity android:name="android.friendr.LoginActivity" />
        <activity android:name="android.friendr.Index">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>